<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MdlRegistrationHubs" table="mdl_registration_hubs">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="token" type="string" column="token" length="255" nullable="false"/>
    <field name="hubname" type="string" column="hubname" length="255" nullable="false"/>
    <field name="huburl" type="string" column="huburl" length="255" nullable="false"/>
    <field name="confirmed" type="boolean" column="confirmed" nullable="false"/>
    <field name="secret" type="string" column="secret" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
