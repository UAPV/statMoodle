<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MdlReferentiel" table="mdl_referentiel">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="80" nullable="false"/>
    <field name="descriptionInstance" type="text" column="description_instance" nullable="false"/>
    <field name="labelDomaine" type="string" column="label_domaine" length="80" nullable="false"/>
    <field name="labelCompetence" type="string" column="label_competence" length="80" nullable="false"/>
    <field name="labelItem" type="string" column="label_item" length="80" nullable="false"/>
    <field name="dateInstance" type="bigint" column="date_instance" nullable="false"/>
    <field name="course" type="bigint" column="course" nullable="false"/>
    <field name="refReferentiel" type="bigint" column="ref_referentiel" nullable="false"/>
    <field name="visible" type="boolean" column="visible" nullable="false"/>
    <field name="config" type="string" column="config" length="255" nullable="false"/>
    <field name="configImpression" type="string" column="config_impression" length="255" nullable="false"/>
    <field name="intro" type="text" column="intro" nullable="false"/>
    <field name="introformat" type="smallint" column="introformat" nullable="false"/>
    <field name="maxbytes" type="bigint" column="maxbytes" nullable="false"/>
  </entity>
</doctrine-mapping>
