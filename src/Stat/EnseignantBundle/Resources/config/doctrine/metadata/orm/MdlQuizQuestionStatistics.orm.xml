<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MdlQuizQuestionStatistics" table="mdl_quiz_question_statistics">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="quizstatisticsid" type="bigint" column="quizstatisticsid" nullable="false"/>
    <field name="questionid" type="bigint" column="questionid" nullable="false"/>
    <field name="slot" type="bigint" column="slot" nullable="true"/>
    <field name="subquestion" type="smallint" column="subquestion" nullable="false"/>
    <field name="s" type="bigint" column="s" nullable="false"/>
    <field name="effectiveweight" type="decimal" column="effectiveweight" precision="15" scale="5" nullable="true"/>
    <field name="negcovar" type="boolean" column="negcovar" nullable="false"/>
    <field name="discriminationindex" type="decimal" column="discriminationindex" precision="15" scale="5" nullable="true"/>
    <field name="discriminativeefficiency" type="decimal" column="discriminativeefficiency" precision="15" scale="5" nullable="true"/>
    <field name="sd" type="decimal" column="sd" precision="15" scale="10" nullable="true"/>
    <field name="facility" type="decimal" column="facility" precision="15" scale="10" nullable="true"/>
    <field name="subquestions" type="text" column="subquestions" nullable="true"/>
    <field name="maxmark" type="decimal" column="maxmark" precision="12" scale="7" nullable="true"/>
    <field name="positions" type="text" column="positions" nullable="true"/>
    <field name="randomguessscore" type="decimal" column="randomguessscore" precision="12" scale="7" nullable="true"/>
  </entity>
</doctrine-mapping>
