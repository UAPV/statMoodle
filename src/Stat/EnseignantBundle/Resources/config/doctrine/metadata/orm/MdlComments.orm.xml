<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MdlComments" table="mdl_comments">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="contextid" type="bigint" column="contextid" nullable="false"/>
    <field name="commentarea" type="string" column="commentarea" length="255" nullable="false"/>
    <field name="itemid" type="bigint" column="itemid" nullable="false"/>
    <field name="content" type="text" column="content" nullable="false"/>
    <field name="format" type="boolean" column="format" nullable="false"/>
    <field name="userid" type="bigint" column="userid" nullable="false"/>
    <field name="timecreated" type="bigint" column="timecreated" nullable="false"/>
  </entity>
</doctrine-mapping>
