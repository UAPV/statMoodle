    <field name="customdec1" type="decimal" column="customdec1" precision="12" scale="7" nullable="true"/>
    <field name="customdec2" type="decimal" column="customdec2" precision="12" scale="7" nullable="true"/>
    <field name="customtext1" type="text" column="customtext1" nullable="true"/>
    <field name="customtext2" type="text" column="customtext2" nullable="true"/>
    <field name="timecreated" type="bigint" column="timecreated" nullable="false"/>
    <field name="timemodified" type="bigint" column="timemodified" nullable="false"/>
