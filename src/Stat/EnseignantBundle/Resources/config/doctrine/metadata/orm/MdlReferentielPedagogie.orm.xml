<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MdlReferentielPedagogie" table="mdl_referentiel_pedagogie">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="promotion" type="string" column="promotion" length="4" nullable="true"/>
    <field name="numGroupe" type="string" column="num_groupe" length="20" nullable="true"/>
    <field name="dateCloture" type="string" column="date_cloture" length="120" nullable="true"/>
    <field name="formation" type="string" column="formation" length="40" nullable="true"/>
    <field name="pedagogie" type="string" column="pedagogie" length="40" nullable="true"/>
    <field name="composante" type="string" column="composante" length="40" nullable="true"/>
    <field name="commentaire" type="string" column="commentaire" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
