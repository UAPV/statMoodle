<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MdlMnetApplication" table="mdl_mnet_application">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="50" nullable="false"/>
    <field name="displayName" type="string" column="display_name" length="50" nullable="false"/>
    <field name="xmlrpcServerUrl" type="string" column="xmlrpc_server_url" length="255" nullable="false"/>
    <field name="ssoLandUrl" type="string" column="sso_land_url" length="255" nullable="false"/>
    <field name="ssoJumpUrl" type="string" column="sso_jump_url" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
